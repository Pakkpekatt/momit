<h2><%= @title %></h2>

<table>
    <thead>
        <th>Username</th>
        <th>Email</th>
        <th>Role</th>
        <th>Login Count</th>
        <th>Last Login</th>
        <th>Created</th>
    </thead>
    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
            <td>Role goes here</td>
            <td>Login count here</td>
            <td>Last login date here</td>
            <td><%= user.created_at %></td>
        </tr>
        <% end %>
    </tbody>
</table>